{
  "name": "scrapper-gcenter",
  "version": "2.1.0",
  "description": "Garitacenter api/scripts",
  "private": true,
  "main": "node --use_strict ./dist/server.js",
  "scripts": {
    "start": "node --use_strict ./dist/server.js",
    "unittest": "./node_modules/.bin/mocha --compilers js:babel-core/register --recursive test",
    "test": "npm run lint",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec --compilers js:babel-core/register --recursive test",
    "webpack": "webpack --progress --colors",
    "webpack-cron": "webpack --progress --colors --config webpack.config.cron.js",
    "dev": "nodemon dist/server.js",
    "cron": "node build/main.js",
    "lint": "./node_modules/.bin/eslint .",
    "update": "git checkout master && git pull origin develop",
    "deploy": "git add . && git commit -m 'deploy' && git push upstream HEAD:master",
    "build": "./node_modules/.bin/babel src -d dist",
    "docker:build": "yarn build && docker build -t garciadiazjaime/api-gcenter .",
    "docker:run": "docker run -d -p 49182:3030 --link mongo -e='DB_URL=mongodb://mongo/gcenter' --name=api-gcenter garciadiazjaime/api-gcenter",
    "docker:push": "docker push garciadiazjaime/api-gcenter",
    "docker:pull": "docker pull garciadiazjaime/api-gcenter"
  },
  "keywords": [],
  "author": "garciadiazjaime@gmail.com",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "body-parser": "^1.15.2",
    "convict": "^1.4.0",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "express": "^4.14.0",
    "googleanalytics": "^0.3.7",
    "lodash": "^4.13.1",
    "loggly": "^1.1.0",
    "moment": "^2.14.1",
    "mongodb": "^2.1.18",
    "mongoose": "^5.0.7",
    "morgan": "^1.9.0",
    "newrelic": "^1.29.0",
    "request": "^2.72.0",
    "twitter": "^1.4.0",
    "winston-loggly": "^1.3.1",
    "xml2js": "^0.4.16"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "istanbul": "^0.4.4",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1"
  }
}
