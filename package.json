    "d3-node": "^2.2.1",
    "d3-scale": "^3.2.1",
    "d3node-barchart": "^0.1.1",
    "express": "^4.14.0",
    "fb": "^2.0.0",
    "i": "^0.3.6",
    "json2csv": "^4.5.4",
    "lodash": "^4.17.15",
    "moment": "^2.14.1",
    "moment-timezone": "^0.5.27",
    "mongodb": "^3.4.0",
